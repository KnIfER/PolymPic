<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="app">
        <attr name="contentInsetStartWithNavigation" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="MaterialScrollBar">
        <attr name="msb_recyclerView" format="reference"/>
        <attr name="msb_lightOnTouch" format="boolean|reference"/>
        <attr name="msb_handleColor" format="color|reference"/>
        <attr name="msb_barColor" format="color|reference"/>
        <attr name="msb_handleOffColor" format="color|reference"/>
        <attr name="msb_textColor" format="color|reference"/>
        <attr name="msb_barThickness" format="dimension|reference"/>
        <attr name="msb_rightToLeft" format="boolean|reference"/>
    </declare-styleable>

    <declare-styleable name="CheckableImageview">
        <attr name="CheckedTint" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="DescriptiveImageView">
        <attr name="android:text" format="string|reference"/>
        <attr name="android:layout_y" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="TouchScrollBar">
        <attr name="msb_hideDelayInMilliseconds" format="integer|reference"/>
        <attr name="msb_autoHide" format="boolean|reference"/>
    </declare-styleable>

	<!-- Defines the custom XML attributes supported for a HorizontalListView -->
	<declare-styleable name="HorizontalListView">
		<attr name="dividerWidth" format="dimension" />
		<attr name="android:divider" />
		<attr name="android:requiresFadingEdge" />
		<attr name="android:fadingEdgeLength" />
	</declare-styleable>


	<declare-styleable name="SplitView">
        <attr name="handle" format="reference" />
        <attr name="primaryContent" format="reference"/>
        <attr name="secondaryContent" format="reference"/>
        
        <attr name="CompensationTop" format="dimension"/>
        <attr name="CompensationBottom" format="dimension"/>
        <attr name="ReservationLeft" format="dimension"/>
        <attr name="ReservationRight" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="DynListPref">
        <attr name="key" format="string" />
        <attr name="summary" format="string"/>
        <attr name="title" format="string"/>
    </declare-styleable>
    
    
    
    <declare-styleable name="UIStyles">
        <attr name="dim0" format="dimension"/>
        <attr name="dim1" format="dimension"/>
    </declare-styleable>


	<attr format="reference" name="ic_launch"/>


	<declare-styleable name="constances">
        <attr name="_0" format="integer" />
    </declare-styleable>
    
    <declare-styleable name="single">
        <attr name="s0" format="reference|color" />
    </declare-styleable>
    
    <declare-styleable name="dual">
        <attr name="d0" format="integer" />
        <attr name="d1" format="integer" />
    </declare-styleable>
    
   <declare-styleable name="triple">
        <attr name="t1" format="integer" />
        <attr name="t2" format="integer" />
        <attr name="t3" format="integer" />
    </declare-styleable>
	<declare-styleable name="ListView">
		<attr name="android:divider" format="reference" />
		<attr name="android:overScrollHeader" format="reference" />
		<attr name="android:overScrollFooter" format="reference" />
		<attr name="android:dividerHeight" format="dimension" />
		<attr name="android:headerDividersEnabled" format="boolean" />
		<attr name="android:footerDividersEnabled" format="boolean" />
	</declare-styleable>

	<declare-styleable name="RecyclerViewPager">
		<attr name="triggerOffset" format="float" />
		<attr name="flingFactor" format="float" />
	</declare-styleable><!-- end recyclerViewpater -->

	<declare-styleable name="FlowTextView">
		<attr name="android:src" format="reference" />
		<attr name="android:textSize" format="dimension" />
		<attr name="android:textColor" format="dimension" />
		<attr name="android:lineSpacingExtra" format="dimension" />
		<attr name="android:lineSpacingMultiplier" format="float" />
		<attr name="starWidth" format="dimension" />
		<attr name="android:text" format="string" />
		<attr name="android:maxLines" format="integer" />
		<attr name="android:gravity" format="enum"/>
		<attr name="margin" format="dimension" />
		<attr name="rating" format="boolean" />
	</declare-styleable>
</resources>